{
  "name": "react-skeleton",
  "version": "1.0.0",
  "description": "Skeleton project for React",
  "main": "src/index.js",
  "scripts": {
    "dist": "npm run test && webpack -p --config webpack.prod.js",
    "start": "webpack-dev-server --open --config webpack.dev.js --progress",
    "test": "jest && eslint --ext .js,.jsx src/* __tests__/*",
    "watch": "jest --watchAll",
    "lint": "eslint --fix --ext .js,.jsx src/* __tests__/*"
  },
  "repository": "https://github.com/ddubson/react-webpack-babel",
  "author": "Dmitriy Dubson",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "immutability-helper": "^2.6.6",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.10",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-react-16-adapter-setup": "^0.1.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "html-webpack-plugin": "^3.2.0",
    "inline-source-map": "^0.6.2",
    "jest": "^24.0.0",
    "node-sass": "^7.0.0",
    "react-test-renderer": "16.0.0",
    "redux-devtools": "^3.4.1",
    "sass-loader": "^6.0.6",
    "source-map": "^0.7.0",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      "@babel/plugin-proposal-json-strings"
    ]
  },
  "jest": {
    "bail": true,
    "setupFiles": [
      "enzyme-react-16-adapter-setup"
    ],
    "testURL": "http://localhost"
  }
}
